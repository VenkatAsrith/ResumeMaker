# Server Configuration
PORT=5000
NODE_ENV=production

# MongoDB Atlas Connection String
# Replace with your actual MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/resume_maker?retryWrites=true&w=majority

# JWT Secret - Use a strong, unique secret in production
# Generate one using: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your_super_secret_jwt_key_at_least_32_characters_long

# Frontend URL for CORS (update this when deploying frontend)
FRONTEND_URL=http://localhost:5173
